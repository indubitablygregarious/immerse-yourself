{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Immerse Yourself",
  "version": "0.3.25",
  "identifier": "com.peterlesko.immerseyourself",
  "build": {
    "frontendDist": "./ui/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Immerse Yourself",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ],
    "resources": {
      "../../env_conf": "env_conf",
      "../../sound_conf": "sound_conf",
      "../../sounds": "sounds",
      "../../freesound_sounds/**/*": "freesound_sounds/"
    }
  }
}
